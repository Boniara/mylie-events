<md-dialog aria-label="Mango (Fruit)">
    <form ng-cloak style="margin-bottom: 0px">
        <md-toolbar>
            <div class="md-toolbar-tools">
                <h2>Photo</h2>
                <span flex></span>
                <md-button aria-label="cancel button" class="md-icon-button" ng-click="cancel()">
                    <i class="fa fa-times" aria-hidden="true"></i>
                </md-button>
            </div>
        </md-toolbar>

        <md-dialog-content>
            <div>
                <img ng-src="/resources/images/1.jpg"/>
            </div>
        </md-dialog-content>
    </form>
</md-dialog>